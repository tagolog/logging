<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>Tagolog</id>
        <version>1.0</version>
        <title>Tagolog</title>
        <authors>w@tagolog.com</authors>
        <owners>w@tagolog.com</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <licenseUrl>https://raw.github.com/tagolog/Logging/master/license.txt</licenseUrl>
        <projectUrl>https://github.com/tagolog/Logging</projectUrl>
        <iconUrl>https://raw.github.com/tagolog/Logging/master/NuGet/Icons/Tag-O-Log.512x512.ico</iconUrl>
        <repository type="git" url="https://github.com/tagolog/logging.git" />
        <description>Tagolog is a structured logging library for .NET. It can help you produce log entries marked with tags easily.
Supported platforms:
- .NET Framework 3.5+;
- .NET Standard 2.0+;
</description>
        <copyright>Copyright (c) 20016-2019 Tagolog Project - https://github.com/tagolog/Logging</copyright>
        <tags>logging log tracing logfile structured tag</tags>
        <dependencies>
            <group targetFramework=".NETFramework3.5" />
            <group targetFramework=".NETStandard2.0" />
        </dependencies>
    </metadata>
    <files>
        <file src="bin/Release/net3.5/Tagolog.dll" target="lib\net35\" />
        <file src="bin/Release/net3.5/Tagolog.pdb" target="lib\net35\" />
        <file src="bin/Release/netstandard2.0/Tagolog.dll" target="lib\netstandard2.0\" />
        <file src="bin/Release/netstandard2.0/Tagolog.pdb" target="lib\netstandard2.0\" />
    </files>
</package>
